<Window x:Class="Window7"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppVb"
        mc:Ignorable="d"
        Title="Window7" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="103,105,364,254">
            <local:TagBar x:Name="tagBar" HorizontalAlignment="Left" VerticalAlignment="Stretch">
                <local:TagBar.Tags>
                    <local:TagNote/>
                </local:TagBar.Tags>
            </local:TagBar>
        </StackPanel>
        <Button Content="Button" HorizontalAlignment="Left" Margin="466,81,0,0" VerticalAlignment="Top" Width="75" Click="AddTags"/>
        <Rectangle Fill="#FF5EBB4A" HorizontalAlignment="Left" Height="24" Margin="625,100,0,0" Stroke="Black" VerticalAlignment="Top" Width="28"/>
    </Grid>
</Window>
